<script>


let btns = document.querySelectorAll("#taxes");
btns.forEach(btn => {
    btn.addEventListener("click", ()=>{
        let gst = document.querySelectorAll("#gst");
        gst.forEach(element => {
            element.style.display = gst.length? "initial" : "none";
        });
    });
});

